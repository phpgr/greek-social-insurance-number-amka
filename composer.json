{
    "name": "phpgr/greek_social_insurance_number",
    "description": "A value object class to for Greek Social Insurance Number (AMKA)",
    "license": "MIT",
    "homepage": "https://github.com/phpgr/php_valid_AMKA",
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GreekSocialInsuranceNumber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GreekSocialInsuranceNumber\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit --colors",
        "phpcs": "php vendor/bin/phpcs src tests",
        "phpcbf": "php vendor/bin/phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
